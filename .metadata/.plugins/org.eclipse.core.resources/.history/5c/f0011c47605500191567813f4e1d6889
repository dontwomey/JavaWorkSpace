
public class PrimeNumber {

	public static void main(String[] args) {
		
		int num = 11;
		boolean primeCheck = isPrime(num);
		System.out.println(primeCheck);

	}

	private static boolean isPrime(int num) {
		
		boolean flag = false;
		for (int i = 2; i <= num/2; i++){
			if ((num % i) == 0 );
			flag = true;
		}
		return flag;
	}

}
